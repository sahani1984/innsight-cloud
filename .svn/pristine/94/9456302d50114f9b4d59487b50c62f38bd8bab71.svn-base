.charts{
    width:100%; height:auto; min-height:300px; display:block; float: left; clear:both;  position: relative;
    &:after{content:""; background-color:#fff; position: absolute; right: -10px; height: 100%; width: 10px;top: 0;}
    highcharts-chart{width: 100%; height: 300px; display: block; font-family: 'Poppins, Helvetica, sans-serif !important'}
  }
  .mat-expansion-panel-header.mat-expanded{max-height:50px;}
  .searching{width:100%; display:flex; clear:both; min-height:300px; position: relative; align-items:center; justify-content:center; height:300px; padding-bottom:50px;
    .cloud_spinner_dark{
        &::before{width:100px; height:100px; border:2px solid #999; border-right:5px solid transparent;}
        &::after{content: "Loading..."; position: absolute;left: 14px;top: 30px; font-style: normal;
          letter-spacing: 2px; color:#999;}
        }
      }